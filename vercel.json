{
  "version": 2,
  "rewrites": [
    { "source": "/", "destination": "/api/index" },
    { "source": "/swagger-ui-bundle.js", "destination": "https://cdnjs.cloudflare.com/ajax/libs/swagger-ui/5.24.0/swagger-ui-bundle.min.js" },
    { "source": "/swagger-ui-standalone-preset.js", "destination": "https://cdnjs.cloudflare.com/ajax/libs/swagger-ui/5.24.0/swagger-ui-standalone-preset.min.js" },
    { "source": "/(.*)", "destination": "/api/index" }
  ]
}
